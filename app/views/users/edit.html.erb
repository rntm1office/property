<div class="content_title">
  <h2>Editing User Details</h2>
</div>
<div class="options">
  <%= link_to image_tag('view.gif',:size=>'18x18') + ' Show', @user %>
</div>
<% form_for(@user) do |f| %>

  <%= render :partial=>'form',:locals=>{:f=>f} %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

