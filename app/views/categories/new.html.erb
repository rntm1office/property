<div class="content_title">
  <h2>New Category</h2>
</div>
<div class="options">
  <%= link_to image_tag("edit.png",:size=>"18x18")+'Categories', :controller=>"categories",:action=>"index" %>
</div>
<% form_for(@category) do |f| %>
  
  <%= render :partial=>'form',:locals=>{:f=>f}  %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>