<div class="content_title">
  <h2>Editing Category</h2>
</div>
<div class="options">
  <%= link_to image_tag("add.png", :size=>"18x18") + 'Show', @category %>
</div>
<% form_for(@category) do |f| %>
    <%= render :partial=>'form',:locals=>{:f=>f}  %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>