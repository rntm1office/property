<div class="content_title">
  <h2>Editing Transaction</h2>
</div>
<div class="options">
  <%= link_to image_tag("view.gif",:size=>"18x18") + "Show", @transaction %>
</div>
<% form_for(@transaction) do |f| %>
  <%= render :partial=>'form',:locals=>{:f=>f}  %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>